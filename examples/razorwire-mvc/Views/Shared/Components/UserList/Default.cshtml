@model IEnumerable<string>

<div id="active-user-list">
    <h6>Active Users</h6>
    <ul id="user-list-items" class="list-group list-group-flush">
        @if (Model != null)
        {
            @foreach (var user in Model)
            {
                <li id="user-@user" class="list-group-item py-1">@user</li>
            }
        }
    </ul>
    @if (Model == null || !Model.Any())
    {
        <p id="user-list-empty" class="text-muted small">No active users yet.</p>
    }
</div>
