@model IEnumerable<string>

<div id="user-list">
    <h6>Active Users</h6>
    @if (Model?.Any() == true)
    {
        <ul class="list-group list-group-flush">
            @foreach (var user in Model)
            {
                <li class="list-group-item py-1">@user</li>
            }
        </ul>
    }
    else
    {
        <p class="text-muted small">No active users yet.</p>
    }
</div>
