@model IEnumerable<RazorWireWebExample.Services.UserPresenceInfo>

<div id="active-user-list">
    <h6>Active Users</h6>
    <ul id="user-list-items" class="list-group list-group-flush">
        @if (Model != null)
        {
            @foreach (var user in Model)
            {
                await Html.RenderPartialAsync("Components/UserList/_UserItem", user);
            }
        }
    </ul>
    @if (Model == null || !Model.Any())
    {
        <p id="user-list-empty" class="text-muted small">No active users yet.</p>
    }
</div>
